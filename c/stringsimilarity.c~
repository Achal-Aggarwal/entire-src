//sg
#include<stdio.h>
#include<string.h>
int compare(char * str,int l,int offset)
{
int i=0,score=0;
while(((i+offset)<l)&&(str[i]==str[i+offset]))
//if first one fails , array index is never accessed, no chance of a seg fault
{score++;i++;
}
return score;
}
void main(void)
{
char str[100000];
int l,score=0,offset=1,toffset,i=0,cases=0;
scanf("%d",&cases);
while(cases--)
{
score=0,offset=1;
scanf("%s",str);
l=strlen(str);
score=l;
while(offset<l)
{
//score+=compare(str,l,offset);
toffset=offset;
i=0;
while(((i+toffset)<l)&&(str[i]==str[i+toffset]))
//if first one fails , array index is never accessed, no chance of a seg fault
{score++;i++;
toffset++;
}
offset++;
}
printf("\n%d",score);
}
}
