//sg
#include<stdio.h>
#include<string.h>

main()
{
 char N[100000]; 
 int l,lcount[10],count=0,cases;
 int i,t;
 scanf("%d",&cases);
 for(t=0;t<cases;t++)
 {
scanf("%s",N);
// cin>>N;
 l=strlen(N);
 for( i=0;i<l;i++)
 {
   if((N[i]!='7')&&(N[i]!='4'))
   {
     count++;
   }
 }
 lcount[t]=count;
 count=0;
  }
  
 for( t=0;t<cases;t++)
 {
   printf("%d\n",lcount[t]);
 }
 return 0;
}